{
  "name": "load-svg-file",
  "version": "0.0.0",
  "description": "Load SVG files over XHR and embed the SVG content into the DOM.",
  "main": "dist/load-svg-file.commonjs.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "gulp": "gulp --gulpfile task/index.js",
    "build": "npm run gulp -- build",
    "serve": "npm run gulp -- serve",
    "check": "standard",
    "fix": "standard --fix",
    "test": "npm-run-all test-type-*",
    "test-ci": "npm run build && npm run test",
    "test-type-es5": "nyc --report-dir coverage/es5 mocha test/headless/index.js --type-es5",
    "test-type-es5-min": "nyc --report-dir coverage/es5-min mocha test/headless/index.js --type-es5-min",
    "test-type-es6": "nyc --report-dir coverage/es6 mocha test/headless/index.js --type-es6",
    "test-type-amd": "nyc --report-dir coverage/amd mocha test/headless/index.js --type-amd",
    "test-type-amd-min": "nyc --report-dir coverage/amd-min mocha test/headless/index.js --type-amd-min",
    "test-type-commonjs": "nyc --report-dir coverage/commonjs mocha test/headless/index.js --type-commonjs",
    "test-type-umd-browser": "nyc --report-dir coverage/umd-browser mocha test/headless/index.js --type-umd-browser",
    "test-type-umd-browser-min": "nyc --report-dir coverage/umd-browser-min mocha test/headless/index.js --type-umd-browser-min",
    "test-type-umd-amd": "nyc --report-dir coverage/umd-amd mocha test/headless/index.js --type-umd-amd",
    "test-type-umd-amd-min": "nyc --report-dir coverage/umd-amd-min mocha test/headless/index.js --type-umd-amd-min",
    "test-type-umd-commonjs": "nyc --report-dir coverage/umd-commonjs mocha test/headless/index.js --type-umd-commonjs",
    "test-type-umd-commonjs-min": "nyc --report-dir coverage/umd-commonjs-min mocha test/headless/index.js --type-umd-commonjs-min",
    "coverage": "npm run gulp -- coveralls"
  },
  "standard": {
    "globals": [
      "loadSvgFile",
      "XMLHttpRequest",
      "Container",
      "describe",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "it",
      "assert"
    ],
    "ignore": [
      "dist/*.*",
      "src/tmpl/*.*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atjse/load-svg-file.git"
  },
  "keywords": [
    "svg",
    "load",
    "file",
    "xhr",
    "dom",
    "icon",
    "icons",
    "sprite",
    "stack",
    "inline",
    "use",
    "xlink",
    "href",
    "loader",
    "loading"
  ],
  "author": "Richard Szakacs, richardszkcs@gmail.com, www.richardszkcs.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atjse/load-svg-file/issues"
  },
  "homepage": "https://github.com/atjse/load-svg-file#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "connect": "^3.6.6",
    "coveralls": "^3.0.1",
    "dentist": "^1.0.3",
    "get-port": "^3.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-coveralls": "^0.1.4",
    "gulp-debug": "^4.0.0",
    "gulp-header": "^2.0.5",
    "gulp-rename": "^1.2.3",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tap": "^1.0.1",
    "gulp-uglify": "^3.0.0",
    "livereload": "^0.7.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "puppeteer": "^1.5.0",
    "puppeteer-to-istanbul": "^1.2.2",
    "require1k": "^2.0.0",
    "serve-static": "^1.13.2",
    "standard": "^11.0.1"
  }
}
